{
  "name": "scraper",
  "private": true,
  "version": "1.0.0",
  "description": "A web-based tool for scraping carrier data",
  "author": {
    "name": "Scraper Developer"
  },
  "type": "module",
  "main": "server.js",
  "scripts": {
    "dev:client": "vite",
    "dev:server": "node --watch server.js src/utils/**/*.js",
    "dev": "concurrently -k \"npm run dev:client\" \"npm run dev:server\"",
    "build": "vite build",
    "build:secure": "vite build --config vite.config.prod.js",
    "start": "node server.js",
    "start:prod": "NODE_ENV=production node server.js",
    "deploy": "npm run build:secure && npm run start:prod",
    "lint": "eslint .",
    "preview": "vite preview"
  },
  "dependencies": {
    "cheerio": "^1.0.0",
    "cors": "^2.8.5",
    "dotenv": "^16.5.0",
    "express": "^4.21.2",
    "https-proxy-agent": "^7.0.6",
    "json2csv": "^6.0.0-alpha.2",
    "node-fetch": "^3.3.2",
    "react": "^19.1.0",
    "react-dom": "^19.1.0",
    "react-router-dom": "^7.6.0",
    "socks-proxy-agent": "^8.0.5"
  },
  "devDependencies": {
    "@eslint/js": "^9.25.0",
    "@types/react": "^19.1.2",
    "@types/react-dom": "^19.1.2",
    "@vitejs/plugin-react": "^4.4.1",
    "autoprefixer": "^10.4.15",
    "concurrently": "^9.1.2",
    "eslint": "^9.25.0",
    "eslint-plugin-react-hooks": "^5.2.0",
    "eslint-plugin-react-refresh": "^0.4.19",
    "globals": "^16.0.0",
    "postcss": "^8.4.31",
    "tailwindcss": "^3.3.3",
    "terser": "^5.39.2",
    "vite": "^6.3.5"
  }
}
